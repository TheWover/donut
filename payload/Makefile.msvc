payload:
	cl -DBYPASS_AMSI_A -DBYPASS_WLDP_A -Zp8 -c -nologo -Os -O1 -Gm- -GR- -EHa -Oi -GS- -Gy -I ..\include payload.c ..\hash.c ..\encrypt.c clib.c 
	link -nologo -order:@order.txt -entry:ThreadProc -fixed -subsystem:console -nodefaultlib payload.obj hash.obj encrypt.obj clib.obj
	exe2h\exe2h payload.exe
debug:
	cl -DBYPASS_AMSI_A -DBYPASS_WLDP_A -nologo -Zp8 -Gy -DDEBUG -I ..\include payload.c ..\hash.c ..\encrypt.c clib.c
clean:
	del *.obj payload.exe
